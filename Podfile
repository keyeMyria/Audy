# Uncomment the next line to define a global platform for your project
use_frameworks!

def shared_pods
  pod 'RxSwift',    '~> 3.0'
  pod 'RxCocoa',    '~> 3.0'
  pod 'SDWebImage', '~> 4.0'
end

target 'Audy' do

  platform :ios, '10.0'

  shared_pods
  pod 'SwiftyJSON'
  pod 'SnapKit', '~> 3.0'
  pod 'Alamofire', '~> 4.4'

end

target 'Watch Extension' do
  
  platform :watchos, '3.0'
  
  shared_pods

end


